<resources>

    <!--  Poppins Font-->
    <!--Poppins Regular Font-->
    <style name="TEXT_REGULAR_GREY_15" parent="@style/TextAppearance.MaterialComponents.Subtitle1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>

    <style name="TEXT_BOLD_BLACK_20" parent="@style/TextAppearance.MaterialComponents.Headline1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_15ssp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:gravity">center</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="fontFamily">@font/poppins_bold</item>
    </style>



    <style name="start_gl">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="layout_constraintGuide_percent">@integer/gl_left</item>
    </style>

    <style name="end_gl">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="layout_constraintGuide_percent">@integer/gl_right</item>
    </style>

    <style name="bottom_gl">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="layout_constraintGuide_percent">@integer/gl_bottom</item>
    </style>
    <style name="top_gl">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="layout_constraintGuide_percent">@integer/gl_top</item>
    </style>



</resources>
